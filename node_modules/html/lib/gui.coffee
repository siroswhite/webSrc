exports.makeSelectBox = (name, list) ->
  html_selectBoxOption = []
  for index, data of list
    html_selectBoxOption.push(
      ["option", {value: data}, data]
    )
  [
    ["select", {id: name}]
    html_selectBoxOption
  ]

exports.makeSliderWithInput = (areaName, uiName) ->
  {
    slider: ["div", {class:areaName + "Range slider", id:uiName + "_range"}]
    input: ["input", {class:areaName + "RangeText", id:uiName + "_rangeInput", type:"text", size: "4"}]
  }